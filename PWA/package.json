{
  "name": "snartnet-pwa",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "SnartNet Progressive Web App - Decentralized social media client",
  "scripts": {
    "dev": "vite",
    "build:assets": "tsx scripts/build-assets.ts",
    "build": "npm run build:assets && tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest"
  },
  "dependencies": {
    "@chainsafe/libp2p-noise": "^17.0.0",
    "@libp2p/gossipsub": "^15.0.3",
    "@libp2p/mplex": "^12.0.2",
    "@libp2p/websockets": "^10.0.2",
    "@libp2p/webtransport": "^6.0.3",
    "@types/qrcode": "^1.5.5",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "events": "^3.3.0",
    "libp2p": "^3.0.2",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "qr-scanner": "^1.4.2",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "zustand": "^4.4.7"
